{
  "name": "dealershipai",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "demo": "pnpm --filter @dealershipai/web dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:migrate": "pnpm --filter @dealershipai/web prisma migrate deploy",
    "db:generate": "pnpm --filter @dealershipai/web prisma generate",
    "db:studio": "pnpm --filter @dealershipai/web prisma studio",
    "db:seed": "tsx scripts/seed-demo.ts",
    "queue:dev": "tsx queue/worker-dev.ts",
    "deploy:vercel": "vercel --prod",
    "deploy:setup": "pnpm db:generate && pnpm db:migrate"
  },
  "dependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bullmq": "^4.15.4",
    "cheerio": "^1.1.2",
    "clsx": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "ioredis": "^5.3.2",
    "jsdom": "^23.0.1",
    "lucide-react": "^0.263.1",
    "next": "14.2.32",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.1",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "zod": "^3.22.4",
    "next-auth": "^4.24.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1"
  },
  "devDependencies": {
    "turbo": "^1.13.4",
    "@types/pg": "^8.10.9",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "dealership",
    "ai",
    "automotive",
    "seo",
    "reputation",
    "multi-tenant",
    "enterprise"
  ]
}
