# Database
DATABASE_URL="postgresql://user:password@localhost:5432/dealershipai"
SUPABASE_URL=""
SUPABASE_ANON_KEY=""
SUPABASE_SERVICE_ROLE_KEY=""

# Redis (optional - will use in-memory cache if not provided)
REDIS_URL=""

# AI Providers
OPENAI_API_KEY=""
ANTHROPIC_API_KEY=""
GOOGLE_AI_API_KEY=""

# Rate Limiting
RATE_LIMIT_ENABLED="true"
RATE_LIMIT_REQUESTS_PER_MINUTE="100"

# Queue Processing
BULL_REDIS_URL=""
QUEUE_CONCURRENCY="5"

# Logging
LOG_LEVEL="info"
LOG_FORMAT="json"

# Security
JWT_SECRET="your-secret-key-here"
SESSION_SECRET="your-session-secret-here"

# Environment
NODE_ENV="development"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Admin
ADMIN_EMAIL="admin@dealershipai.com"

# Cost Controls
MAX_MONTHLY_SPEND="1000"
COST_ALERT_THRESHOLD="800"

# Feature Flags
ENABLE_BATCH_PROCESSING="true"
ENABLE_QUEUE_MONITORING="true"
ENABLE_ADMIN_FEATURES="true"