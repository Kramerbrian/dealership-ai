# DealershipAI Environment Variables
# Copy this file to .env.local and fill in your values

# Core Application
NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here-generate-with-openssl-rand-base64-32

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/dealershipai"

# AI Providers
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Third-party Integrations
AUTOTRADER_API_KEY=your-autotrader-api-key
AUTOTRADER_SECRET=your-autotrader-secret
CARSCOM_API_KEY=your-carscom-api-key
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret

# Monitoring & Logging
SENTRY_DSN=your-sentry-dsn-here
DATADOG_API_KEY=your-datadog-api-key

# Deployment (Production only)
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id

# Security
JWT_SECRET=your-jwt-secret-32-characters-long
ENCRYPTION_KEY=your-encryption-key-32-bytes-base64
